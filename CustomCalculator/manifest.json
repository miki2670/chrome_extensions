{
  "manifest_version": 2,
  "name": "Custom calculator",
  "description": "Quick adding calculator",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["calculator.js"]
  }],
  "permissions": ["activeTab"] 


}
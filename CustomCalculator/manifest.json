{
  "manifest_version": 2,
  "name": "Custom calculator",
  "description": "Quick adding calculator",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
    "matches": ["http:///",
    "https:///"],
    "js": ["calculator.js"]
    }
  ]
}